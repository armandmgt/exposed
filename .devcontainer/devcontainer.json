{
	"name": "Rust",
	"dockerComposeFile": "docker-compose.yml",
	"service": "exposed",
	"overrideCommand": true,
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer",
				"vadimcn.vscode-lldb",
				"ms-azuretools.vscode-docker",
				"humao.rest-client"
			]
		}
	},
	"forwardPorts": [
		8080
	],
	"remoteUser": "root"
}